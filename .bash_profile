# aliases
# in the shell, the text on the left of the equals is automatically read as text on the right. No $ necessary
alias cd..="cd .."
alias l="ls -al"
alias lp="ls -p"
alias h=history
alias pybin=/~/../usr/local/bin

# Navagational aliases
alias toConfig=/~/etc/

# Bash Prompt
# PS1 is the bash prompt
export PS1="\[\033[35m\]\u\[\033[m\]@\[\033[31m\]\h:\[\033[33;1m\]\w\[\033[m\]\$ "
export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad
